<body class="skin-blue">

<header class="navbar navbar-fixed-top" role="banner">
  <div class="container-fluid">
    <div class="navbar-header">
      <a href="/" class="navbar-brand"><%= image_tag('logo.png') %></a>
    </div>
    <ul class="nav navbar-nav navbar-right">
      <li class="user_menu">
        <a href="#" class="dropdown-toggle" data-toggle="dropdown">
          <span class="navbar_el_icon ion-person"></span>&nbsp;&nbsp;<%=current_user.name%> <span id="currentUserIdHidden" style="display:none;"><%= current_user.id%></span><span class="caret"></span>
        </a>
        <ul class="dropdown-menu dropdown-menu-right">
          <!-- Menu Footer-->
          <% if current_user.present? %>
              <li><%= link_to "ログアウト", destroy_user_session_path, method: :delete %></li>
              <!--<li><%#= link_to "ユーザ設定", "/user/edit/#{current_user.id}" %></li>-->
          <% else %>
              <li><%= link_to "ログイン", new_user_session_path %></li>
          <% end %>
          <% s = SystemSetting.find_value('order_lists') %>
          <% if current_user && s %>
              <% if s == UserConfig::ORDER_LIST_UC %>
                  <li><%= link_to "受注ユーザ表示設定", "/user_config/order_list_update" %></li>
              <% elsif s == UserConfig::ORDER_LIST_ADMIN %>
                  <li><%= link_to "受注表示設定", "/user_config/order_list_update" %></li>
              <% end %>
          <% end %>
        </ul>
      </li>
    </ul>
  </div>
</header>


<div id="main_wrapper">
  <!-- Right side column. Contains the navbar and content of the page -->
  <aside class="right-side">
    <!-- Content Header (Page header) -->
    <%= render :partial => "layouts/message" %>
    <!-- Main content -->
    <section class="content">

      <%= yield %>

    </section>
    <!-- /.content -->

  </aside>
  <!-- /.right-side -->
</div>
<!-- ./wrapper -->
<!-- Left side column. contains the logo and sidebar -->
